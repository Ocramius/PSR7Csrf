    "require": {
        "php":                             "^7.1.0",
        "psr/http-message":                "^1.0.1",
        "lcobucci/jwt":                    "^3.2.2",
        "zendframework/zend-stratigility": "^1.3.3",
        "psr7-sessions/storageless":       "^4.0.0",
        "ocramius/psr7-session":           "^1.0.2 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit":           "^5.3.4",
        "squizlabs/php_codesniffer": "^2.6.0"
    },
    "autoload": {
        "psr-4": {
            "PSR7Csrf\\": "src/PSR7Csrf"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PSR7CsrfTest\\": "test/PSR7CsrfTest"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
